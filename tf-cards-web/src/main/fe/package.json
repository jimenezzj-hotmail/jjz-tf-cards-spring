{
  "name": "fe",
  "version": "1.0.0",
  "description": "This is intended to process tailwind css assets",
  "main": "index.js",
  "scripts": {
    "build-prod": "npx webpack --mode production",
    "dev": "npx webpack --mode development",
    "cp-static": "cp -r \"../resources/templates\" \"../../../target/classes\" && cp -r \"../resources/static/dist\" \"../../../target/classes/static\"",
    "watch": "npm-run-all -p watch:*",
    "watch:static": "npx webpack --mode development && npx webpack w",
    "watch:html": "npx onchange \"../resources/templates/**/*.html\" -- npm run cp-static",
    "watchh:serve": "browser-sync start --no-inject-changes --port 8081 --proxy localhost:8080 --files '../../../target/classes/templates' '../../../target/classes/static' '../resources/templates'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "browser-sync": "^2.28.3",
    "css-loader": "^6.7.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.7",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "webpack": "^5.76.1"
  }
}
